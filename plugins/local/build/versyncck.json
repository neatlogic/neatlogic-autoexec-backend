{
    "opDesc": "主干代码是否已合并",
    "opType": "runner",
    "typeName": "BUILD",
    "riskName": "LOW",
    "interpreter": "perl",
    "defaultProfile": "git-args",
    "description": "检查主干代码是否已经合并到当前版本分支或标签（需要在verget后才能使用）",
    "enabled": 0,
    "option": [{
            "opt": "verbose",
            "name": "显示明细",
            "help": "执行日志显示详细信息",
            "type": "select",
            "required": "true",
            "validate": "",
            "dataSource": {
                "dataList": [{
                        "text": "是",
                        "value": "1"
                    },
                    {
                        "text": "否",
                        "value": "0"
                    }
                ]
            },
            "defaultValue": "1"
        },
        {
            "opt": "lang",
            "name": "语言",
            "help": "工程代码的语言和编码",
            "type": "select",
            "required": "true",
            "validate": "",
            "dataSource": {
                "dataList": [{
                        "text": "en_US.UTF-8",
                        "value": "en_US.UTF-8"
                    },
                    {
                        "text": "en_US.GB18030",
                        "value": "en_US.GB18030"
                    }
                ]
            },
            "defaultValue": "en_US.UTF-8"
        },
        {
            "opt": "startpath",
            "name": "子目录",
            "help": "存放代码仓库的子目录，用于多个代码仓库联合编译的情况",
            "type": "text",
            "required": "false",
            "validate": "",
            "defaultValue": ""
        },
        {
            "opt": "user",
            "name": "用户名",
            "help": "访问仓库的用户名(如GIT使用ssh方式，留空)",
            "type": "text",
            "required": "false",
            "validate": "",
            "defaultValue": "${global:git.user}"
        },
        {
            "opt": "password",
            "name": "密码",
            "help": "访问仓库的密码(如GIT使用ssh方式，留空)",
            "type": "password",
            "required": "false",
            "validate": "",
            "defaultValue": "${global:git.password}"
        }
    ],
    "output": []
}