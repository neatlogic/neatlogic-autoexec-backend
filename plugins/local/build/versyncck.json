{
    "opDesc": "主干代码是否已合并",
    "opType": "runner",
    "typeName": "BUILD",
    "riskName": "LOW",
    "interpreter": "perl",
    "defaultProfile": "git-args",
    "description": "检查主干代码是否已经合并到当前版本分支或标签（需要在verget后才能使用）",
    "enabled": 0,
    "option": [
        {
            "opt": "verbose",
            "name": "显示明细",
            "help": "执行日志显示详细信息",
            "type": "radio",
            "isConst": "false",
            "defaultValue": [
                {
                    "text": "是",
                    "value": "1",
                    "selected": "true"
                },
                {
                    "text": "否",
                    "value": "0"
                }
            ],
            "required": "true",
            "validate": ""
        },
        {
            "opt": "lang",
            "name": "语言",
            "help": "工程代码的语言和编码",
            "type": "select",
            "isConst": "false",
            "defaultValue": [
                {
                    "text": "en_US.UTF-8",
                    "value": "en_US.UTF-8",
                    "selected": "true"
                },
                {
                    "text": "en_US.GB18030",
                    "value": "en_US.GB18030"
                }
            ],
            "required": "true",
            "validate": ""
        },
        {
            "opt": "username",
            "name": "用户名",
            "help": "访问仓库的用户名",
            "type": "text",
            "isConst": "false",
            "defaultValue": "${global:git.user}",
            "required": "true",
            "validate": ""
        },
        {
            "opt": "password",
            "name": "密码",
            "help": "访问仓库的密码",
            "type": "password",
            "isConst": "false",
            "defaultValue": "${global:git.password}",
            "required": "true",
            "validate": ""
        }
    ],
    "output": []
}