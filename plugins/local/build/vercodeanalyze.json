{
    "opDesc": "版本代码变更分析",
    "opType": "runner",
    "typeName": "BUILD",
    "riskName": "LOW",
    "interpreter": "python",
    "defaultProfile": "git-args",
    "description": "版本代码变更分析，对GIT/SVN代码进行diff",
    "enabled": 1,
    "option": [
        {
            "opt": "repotype",
            "name": "仓库类型",
            "help": "代码仓库的类型",
            "type": "select",
            "required": "true",
            "validate": "",
            "dataSource": {
                "dataList": [{
                        "text": "GIT",
                        "value": "GIT"
                    },
                    {
                        "text": "SVN",
                        "value": "SVN"
                    }
                ]
            },
            "defaultValue": "GIT"
        },
        {
            "opt": "repo",
            "name": "仓库地址",
            "help": "GIT的仓库地址或SVN仓库的根路径",
            "type": "text",
            "required": "true",
            "validate": "",
            "defaultValue": ""
        },
        {
            "opt": "user",
            "name": "用户名",
            "help": "访问仓库的用户名(如GIT使用ssh方式，留空)",
            "type": "text",
            "required": "false",
            "validate": "",
            "defaultValue": "${global:git.user}"
        },
        {
            "opt": "password",
            "name": "密码",
            "help": "访问仓库的密码(如GIT使用ssh方式，留空)",
            "type": "password",
            "required": "false",
            "validate": "",
            "defaultValue": "${global:git.password}"
        },
        {
            "opt": "trunk",
            "name": "主干",
            "help": "GIT的主干分支或SVN的主干子目录",
            "type": "text",
            "required": "true",
            "validate": "",
            "defaultValue": "master"
        },
        {
            "opt": "branch",
            "name": "版本分支",
            "help": "GIT的版本分支或SVN的版本子目录",
            "type": "text",
            "required": "true",
            "validate": "",
            "defaultValue": "${VERSION}"
        }
    ],
    "output": []
}