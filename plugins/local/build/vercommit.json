{
    "opDesc": "合并版本代码到主干",
    "opType": "runner",
    "typeName": "BUILD",
    "riskName": "LOW",
    "interpreter": "perl",
    "defaultProfile": "git-args",
    "description": "合并当前版本代码到主干分支（需要在verget后才能使用）",
    "enabled": 0,
    "option": [
        {
            "opt": "verbose",
            "name": "显示明细",
            "help": "执行日志显示详细信息",
            "type": "select",
            "required": "true",
            "validate": "",
            "dataSource": {
                "dataList": [
                    {
                        "text": "是",
                        "value": "1"
                    },
                    {
                        "text": "否",
                        "value": "0"
                    }
                ]
            },
            "defaultValue": "1"
        },
        {
            "opt": "lang",
            "name": "语言",
            "help": "工程代码的语言和编码",
            "type": "select",
            "required": "true",
            "validate": "",
            "dataSource": {
                "dataList": [
                    {
                        "text": "en_US.UTF-8",
                        "value": "en_US.UTF-8"
                    },
                    {
                        "text": "en_US.GB18030",
                        "value": "en_US.GB18030"
                    }
                ]
            },
            "defaultValue": "en_US.UTF-8"
        },
        {
            "opt": "user",
            "name": "用户名",
            "help": "访问仓库的用户名",
            "type": "text",
            "required": "true",
            "validate": "",
            "defaultValue": "${global:git.user}"
        },
        {
            "opt": "password",
            "name": "密码",
            "help": "访问仓库的密码",
            "type": "password",
            "required": "true",
            "validate": "",
            "defaultValue": "${global:git.password}"
        }
    ],
    "output": []
}