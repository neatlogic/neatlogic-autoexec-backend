{
    "opDesc": "编译增量补丁",
    "opType": "runner",
    "typeName": "BUILD",
    "riskName": "LOW",
    "interpreter": "perl",
    "defaultProfile": "maven-args",
    "description": "使用代码仓库的diff功能抽取编译后的文件生成补丁",
    "enabled": 1,
    "option": [{
            "opt": "verbose",
            "name": "显示明细",
            "help": "执行日志显示详细信息",
            "type": "select",
            "required": "true",
            "validate": "",
            "dataSource": {
                "dataList": [{
                        "text": "是",
                        "value": "1"
                    },
                    {
                        "text": "否",
                        "value": "0"
                    }
                ]
            },
            "defaultValue": "1"
        },
        {
            "opt": "lang",
            "name": "语言",
            "help": "工程代码的语言和编码",
            "type": "select",
            "required": "true",
            "validate": "",
            "dataSource": {
                "dataList": [{
                        "text": "en_US.UTF-8",
                        "value": "en_US.UTF-8"
                    },
                    {
                        "text": "en_US.GB18030",
                        "value": "en_US.GB18030"
                    }
                ]
            },
            "defaultValue": "en_US.UTF-8"
        },
        {
            "opt": "type",
            "name": "编译类型",
            "help": "编译类型",
            "type": "select",
            "required": "true",
            "validate": "",
            "dataSource": {
                "dataList": [{
                        "text": "maven",
                        "value": "maven"
                    },
                    {
                        "text": "ant",
                        "value": "ant"
                    },
                    {
                        "text": "gradle",
                        "value": "gradle"
                    },
                    {
                        "text": "npm",
                        "value": "npm"
                    }
                ]
            },
            "defaultValue": "maven"
        },
        {
            "opt": "jdk",
            "name": "JDK版本",
            "help": "JDK版本",
            "type": "select",
            "required": "false",
            "validate": "",
            "dataSource": {
                "dataList": [{
                        "text": "自动",
                        "value": ""
                    },
                    {
                        "text": "jdk1.8",
                        "value": "jdk1.8"
                    },
                    {
                        "text": "jdk1.7",
                        "value": "jdk1.7"
                    },
                    {
                        "text": "jdk1.6",
                        "value": "jdk1.6"
                    },
                    {
                        "text": "jdk1.5",
                        "value": "jdk1.5"
                    }
                ]
            },
            "defaultValue": ""
        },
        {
            "opt": "args",
            "name": "自定义参数",
            "help": "编译工具的自定义参数（譬如：ant或mvn的参数）",
            "type": "text",
            "required": "false",
            "validate": "",
            "defaultValue": ""
        },
        {
            "opt": "startpath",
            "name": "起始子目录",
            "help": "编译配置所在子目录（相对于工程根）",
            "type": "text",
            "required": "false",
            "validate": "",
            "defaultValue": ""
        },
        {
            "opt": "tag4cmpto",
            "name": "基线标签",
            "help": "基线的标签，当前代码与它比较抽取（如果是基于Revision，留空）",
            "type": "text",
            "required": "false",
            "validate": "",
            "defaultValue": ""
        },
        {
            "opt": "startrev",
            "name": "起始Revision",
            "help": "补丁开始的Revision号（如果留空，则根据当前版本的开始Revision号来抽取）",
            "type": "text",
            "required": "false",
            "validate": "",
            "defaultValue": ""
        },
        {
            "opt": "endrev",
            "name": "结束Revision",
            "help": "补丁结束的Revision号（如果留空，则根据当前版本的结束Revision号来抽取）",
            "type": "text",
            "required": "false",
            "validate": "",
            "defaultValue": ""
        },
        {
            "opt": "diffdirname",
            "name": "增量保存目录",
            "help": "相对工程根的子目录，用于保存diff出来的文件和目录",
            "type": "text",
            "required": "true",
            "validate": "",
            "defaultValue": "build.diff"
        },
        {
            "opt": "srcpaths",
            "name": "Java代码目录",
            "help": "例如：src,*/src",
            "type": "text",
            "required": "true",
            "validate": "",
            "defaultValue": "src"
        },
        {
            "opt": "testsrcpaths",
            "name": "测试Java代码目录",
            "help": "例如：src/test",
            "type": "text",
            "required": "false",
            "validate": "",
            "defaultValue": "src/test"
        },
        {
            "opt": "websrcpath",
            "name": "WEB代码目录",
            "help": "存放WEB源码的目录，例如：src/main/webapp",
            "type": "text",
            "required": "true",
            "validate": "",
            "defaultValue": "src/main/webapp"
        },
        {
            "opt": "webdistpath",
            "name": "WEB发布目录",
            "help": "WEB文件最终打包的目录，例如：WebContent",
            "type": "text",
            "required": "true",
            "validate": "",
            "defaultValue": "src/main/webapp"
        },
        {
            "opt": "cmpoutpaths",
            "name": "编译输出目录",
            "help": "例如：target/webapp/WEB-INF/classes,*/target/webapp/WEB-INF/classes",
            "type": "text",
            "required": "true",
            "validate": "",
            "defaultValue": "target/webapp/WEB-INF/classes"
        },
        {
            "opt": "forcecompile",
            "name": "强制编译",
            "help": "不管是否有增量修改，都执行编译",
            "type": "select",
            "required": "true",
            "validate": "",
            "dataSource": {
                "dataList": [{
                        "text": "是",
                        "value": "1"
                    },
                    {
                        "text": "否",
                        "value": "0"
                    }
                ]
            },
            "defaultValue": "0"
        },
        {
            "opt": "islib",
            "name": "是否子模块",
            "help": "当前获取的代码仓库是子模块，用于多个代码仓库联合编译的情况",
            "type": "select",
            "required": "false",
            "validate": "",
            "dataSource": {
                "dataList": [{
                        "text": "否",
                        "value": "0"
                    },
                    {
                        "text": "是",
                        "value": "1"
                    }
                ]
            },
            "defaultValue": "0"
        }
    ],
    "output": []
}