{
    "opDesc": "同步文件时间",
    "opType": "runner",
    "typeName": "BUILD",
    "riskName": "LOW",
    "interpreter": "perl",
    "defaultProfile": null,
    "description": "把源目录下的文件的时间同步到目标目录",
    "enabled": 0,
    "option": [
        {
            "opt": "verbose",
            "name": "显示明细",
            "help": "执行日志显示详细信息",
            "type": "radio",
            "required": "true",
            "validate": "",
            "dataSource": {
                "dataList": [
                    {
                        "text": "是",
                        "value": "1"
                    },
                    {
                        "text": "否",
                        "value": "0"
                    }
                ]
            },
            "defaultValue": "1"
        },
        {
            "opt": "src",
            "name": "源目录",
            "help": "源目录（相对于当前系统存储物料的根目录）",
            "type": "text",
            "required": "true",
            "validate": "",
            "defaultValue": ""
        },
        {
            "opt": "dest",
            "name": "目标目录",
            "help": "目标目录（相对于当前系统存储物料的根目录）",
            "type": "text",
            "required": "true",
            "validate": "",
            "defaultValue": "${global:git.user}"
        }
    ],
    "output": []
}