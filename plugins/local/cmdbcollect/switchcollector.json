{
    "opName": "交换机CMDB采集",
    "opType": "localremote",
    "typeName": "CMDB",
    "riskName": "LOW",
    "interpreter": "perl",
    "description": "采集交换机信息,支持Cisco、HuaWei、H3C等",
    "option": [{
            "opt": "community",
            "name": "默认团体字",
            "help": "当在节点信息中没有密码是，使用此默认SNMP团体字",
            "type": "password",
            "isConst": "false",
            "defaultValue": "",
            "required": "false",
            "validate": ""
        },
        {
            "opt": "timeout",
            "name": "超时时间",
            "help": "单次SNMP查询超时时间，单位秒",
            "type": "text",
            "isConst": "false",
            "defaultValue": "10",
            "required": "false",
            "validate": ""
        },
        {
            "opt": "objtype",
            "name": "设备类型",
            "help": "设备类型，譬如：Switch | Router",
            "type": "select",
            "isConst": "false",
            "defaultValue": [{
                    "text": "交换机",
                    "value": "Switch",
                    "selected": "true"
                },
                {
                    "text": "路由器",
                    "value": "Router"
                }
            ],
            "required": "true",
            "validate": ""
        },
        {
            "opt": "verbose",
            "name": "回显结果数据",
            "help": "回显采集到的数据",
            "type": "select",
            "isConst": "false",
            "defaultValue": [{
                    "text": "不回显",
                    "value": "0",
                    "selected": "true"
                },
                {
                    "text": "回显",
                    "value": "1"
                }
            ],
            "required": "true",
            "validate": ""
        }
    ],
    "output": [{
        "opt": "DATA",
        "name": "JSON数据",
        "help": "采集到的数据",
        "type": "json"
    }]
}