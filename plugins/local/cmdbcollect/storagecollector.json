{
    "opName": "存储CMDB采集",
    "opType": "localremote",
    "typeName": "CMDB",
    "riskName": "LOW",
    "interpreter": "perl",
    "description": "采集存储设备信息,支持IBM、NetApp、EMC、HuaWei等",
    "option": [{
            "opt": "community",
            "name": "默认团体字",
            "help": "当在节点信息中没有密码是，使用此默认SNMP团体字",
            "type": "password",
            "isConst": "false",
            "defaultValue": "",
            "required": "false",
            "validate": ""
        },
        {
            "opt": "timeout",
            "name": "超时时间",
            "help": "查询超时时间，单位秒",
            "type": "text",
            "isConst": "false",
            "defaultValue": "10",
            "required": "false",
            "validate": ""
        },
        {
            "opt": "type",
            "name": "存储类别",
            "help": "存储的类别型号",
            "type": "select",
            "isConst": "false",
            "defaultValue": [{
                    "text": "EMC_Vnx",
                    "value": "EMC_Vnx"
                },
                {
                    "text": "HDS_AMS",
                    "value": "HDS_AMS"
                },
                {
                    "text": "HDS_VSP",
                    "value": "HDS_VSP"
                },
                {
                    "text": "HuaWei",
                    "value": "HuaWei"
                },
                {
                    "text": "IBM_DS",
                    "value": "IBM_DS"
                },
                {
                    "text": "IBM_F900",
                    "value": "IBM_F900"
                },
                {
                    "text": "IBM_Flash",
                    "value": "IBM_Flash"
                },
                {
                    "text": "IBM_SVC",
                    "value": "IBM_SVC"
                },
                {
                    "text": "IBM_V7000",
                    "value": "IBM_V7000"
                },
                {
                    "text": "NetApp",
                    "value": "NetApp"
                },
                {
                    "text": "FUJITSU/浪潮OEM",
                    "value": "FUJITSU"
                }
            ],
            "required": "true",
            "validate": ""
        },
        {
            "opt": "inspect",
            "name": "巡检检查",
            "help": "采集更详细的巡检数据",
            "type": "select",
            "isConst": "false",
            "defaultValue": [{
                    "text": "不采集",
                    "value": "0",
                    "selected": "true"
                },
                {
                    "text": "采集",
                    "value": "1"
                }
            ],
            "required": "true",
            "validate": ""
        },
        {
            "opt": "verbose",
            "name": "回显结果数据",
            "help": "回显采集到的数据",
            "type": "select",
            "isConst": "false",
            "defaultValue": [{
                    "text": "不回显",
                    "value": "0",
                    "selected": "true"
                },
                {
                    "text": "回显",
                    "value": "1"
                }
            ],
            "required": "true",
            "validate": ""
        }
    ],
    "output": [{
        "opt": "DATA",
        "name": "JSON数据",
        "help": "采集到的数据",
        "type": "json"
    }]
}