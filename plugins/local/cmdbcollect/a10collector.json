{
    "opName": "A10CMDB采集",
    "opType": "localremote",
    "typeName": "CMDB",
    "riskName": "LOW",
    "interpreter": "perl",
    "description": "使用SNMP采集A10的VS、POOL、MEMBER信息",
    "option": [{
            "opt": "community",
            "name": "默认团体字",
            "help": "当在节点信息中没有密码是，使用此默认SNMP团体字",
            "type": "password",
            "isConst": "false",
            "defaultValue": "",
            "required": "false",
            "validate": ""
        },
        {
            "opt": "timeout",
            "name": "超时时间",
            "help": "单次SNMP查询超时时间，单位秒",
            "type": "input",
            "isConst": "false",
            "defaultValue": "10",
            "required": "false",
            "validate": ""
        }
    ],
    "output": [{
        "opt": "DATA",
        "name": "JSON数据",
        "help": "采集到的数据",
        "type": "json"
    }]
}