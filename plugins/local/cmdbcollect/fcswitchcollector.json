{
    "opName": "光交信息采集",
    "opType": "localremote",
    "typeName": "CMDB",
    "riskName": "LOW",
    "interpreter": "perl",
    "defaultProfile": null,
    "description": "采集光交设备信息,支持基于Brocade的类型",
    "option": [
        {
            "opt": "timeout",
            "name": "超时时间",
            "help": "查询超时时间，单位秒",
            "type": "text",
            "required": "false",
            "validate": "",
            "defaultValue": "10"
        },
        {
            "opt": "type",
            "name": "光交类别",
            "help": "光交的类别型号",
            "type": "select",
            "required": "true",
            "validate": "",
            "defaultValue": "auto",
            "dataSource": [
                {
                    "text": "自动选择",
                    "value": "auto"
                },
                {
                    "text": "Brocade",
                    "value": "Brocade"
                },
                {
                    "text": "IBM",
                    "value": "Brocade"
                },
                {
                    "text": "EMC",
                    "value": "Brocade"
                },
                {
                    "text": "HP",
                    "value": "Brocade"
                },
                {
                    "text": "Huawei",
                    "value": "Brocade"
                }
            ]
        },
        {
            "opt": "verbose",
            "name": "回显结果数据",
            "help": "回显采集到的数据",
            "type": "select",
            "required": "true",
            "validate": "",
            "defaultValue": "0",
            "dataSource": [
                {
                    "text": "不回显",
                    "value": "0"
                },
                {
                    "text": "回显",
                    "value": "1"
                }
            ]
        }
    ],
    "output": [
        {
            "opt": "DATA",
            "name": "JSON数据",
            "help": "采集到的数据",
            "type": "json"
        }
    ]
}