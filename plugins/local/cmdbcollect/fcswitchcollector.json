            "required": "false",
            "validate": ""
        },
        {
            "opt": "type",
            "name": "光交类别",
            "help": "光交的类别型号",
            "type": "select",
            "isConst": "false",
            "defaultValue": [{
                    "text": "博科",
                    "value": "Brocade"
                },
                {
                    "text": "IBM",
                    "value": "Brocade"
                },
                {
                    "text": "EMC",
                    "value": "Brocade"
                }
            ],
            "required": "true",
            "validate": ""
        }
    ],
    "output": [{
        "opt": "DATA",
        "name": "JSON数据",
        "help": "采集到的数据",
        "type": "json"
    }]
