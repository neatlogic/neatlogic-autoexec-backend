            "help": "NAS地址, 例如: 192.168.0.222:/DataVolume/Public",
            "type": "text",
            "required": "true",
            "validate": "",
            "defaultValue": ""
        },
        {
            "opt": "target",
            "name": "挂载目录",
            "help": "挂载目录，例如：/mnt/nas",
            "type": "text",
            "required": "true",
            "validate": "",
            "defaultValue": ""
        },
        {
            "opt": "username",
            "name": "登录用户",
            "help": "用户名, 如不需要, 留空",
            "type": "text",
            "required": "false",
            "validate": "",
            "defaultValue": ""
        },
        {
            "opt": "password",
            "name": "登录密码",
            "help": "密码, 如不需要, 留空",
            "type": "password",
            "required": "false",
            "validate": "",
            "defaultValue": ""
        },
        {
            "opt": "iocharset",
            "name": "编码",
            "help": "编码, 默认为utf8",
