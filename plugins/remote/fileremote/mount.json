                    },
                    {
                        "text": "utf8",
                        "value": "utf8"
                    },
                    {
                        "text": "gb2312",
                        "value": "gb2312"
                    }
                ]
            },
            "defaultValue": ""
        },
        {
            "opt": "type",
            "name": "文件类型",
            "help": "文件类型",
            "type": "select",
            "required": "false",
            "validate": "",
            "dataSource": {
                "dataList": [{
                        "text": "默认",
                        "value": ""
                    },
                    {
                        "text": "nfs",
                        "value": "nfs"
                    },
                    {
                        "text": "cifs",
                        "value": "cifs"
                    }
                ]
            },
            "defaultValue": ""
        }
    ],
    "output": [{
        "opt": "mountPoint",
        "name": "挂载目录",
        "help": "挂载目录",
        "type": "text"
