{
    "opName": "OS和应用CMDB",
    "opType": "remote",
    "typeName": "CMDB",
    "riskName": "LOW",
    "interpreter": "perl",
    "description": "采集主机硬件、OS和OS上运行应用和DB的信息",
    "option": [{
            "opt": "class",
            "name": "采集类列表",
            "help": "采集类别的名称, 空代表采集所有，逗号相隔，要与采集类名相同，譬如：OS,Tomcat,Weblogic,Oracle",
            "type": "text",
            "isConst": "false",
            "defaultValue": "",
            "required": "false",
            "validate": ""
        },
        {
            "opt": "mysqlaccount",
            "name": "Mysql账户",
            "help": "例如：采集数据需要用到的Mysql的只读用户",
            "type": "account",
            "isConst": "false",
            "defaultValue": "mysqlread/database",
            "required": "false",
            "validate": ""
        },
        {
            "opt": "pgaccount",
            "name": "Postgresql账户",
            "help": "例如：采集数据需要用到的Postgresql的只读用户",
            "type": "account",
            "isConst": "false",
            "defaultValue": "pgread/database",
            "required": "false",
            "validate": ""
        },
        {
            "opt": "mssqlaccount",
            "name": "MSSQLServer账户",
            "help": "例如：采集数据需要用到的MSSQLServer的只读用户",
            "type": "account",
            "isConst": "false",
            "defaultValue": "mssqlread/database",
            "required": "false",
            "validate": ""
        },
        {
            "opt": "defaultpassconf",
            "name": "默认密码设置",
            "help": "例如：Mysql:root/pass1234,Postgresql:demo/test1234",
            "type": "text",
            "isConst": "false",
            "defaultValue": "Mysql:#{mysqlaccount},Postgresql:#{pgaccount},MSSQLServer:#{mssqlaccount}",
            "required": "false",
            "validate": ""
        },
        {
            "opt": "verbose",
            "name": "回显结果数据",
            "help": "回显采集到的数据",
            "type": "select",
            "isConst": "false",
            "defaultValue": [{
                    "text": "不回显",
                    "value": "0",
                    "selected": "true"
                },
                {
                    "text": "回显",
                    "value": "1"
                }
            ],
            "required": "true",
            "validate": ""
        }
    ],
    "output": [{
        "opt": "DATA",
        "name": "JSON数据",
        "help": "采集到的数据",
        "type": "json"
    }]
}