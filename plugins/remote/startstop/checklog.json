    "option": [{
            "opt": "verbose",
            "name": "详细输出",
            "help": "显示匹配的详细信息",
            "type": "radio",
            "isConst": "false",
            "defaultValue": [{
                "text": "是",
                "value": "1",
                "selected": "true"
            }, {
                "text": "否",
                "value": "0"
            }],
            "required": "true",
            "validate": ""
        },
        {
            "opt": "maxsize",
            "name": "最大检查量(MB)",
            "help": "从日志文件末尾开始检查的最大的大小，单位MB",
            "type": "input",
            "isConst": "false",
            "defaultValue": "16",
            "required": "true",
            "validate": ""
        },
        {
            "opt": "failcheck",
            "name": "检测方向",
            "help": "出现关键字是成功还是失败",
