            "required": "true",
            "validate": ""
        },
        {
            "opt": "op",
            "name": "多关键字",
            "help": "多个关键字的操作符号",
            "type": "radio",
            "isConst": "false",
            "defaultValue": [{
                "text": "OR",
                "value": "or",
                "selected": "true"
            }, {
                "text": "AND",
                "value": "and"
            }],
            "required": "true",
            "validate": ""
        }
